import serial
import time
import string
import paho.mqtt.publish as publish

ser = serial.Serial("/dev/rfcomm0", 9600)
ser.write(str.encode('Start\r\n')) #This line will write to arduino

while True:
	if ser.in_waiting > 0:
		rawserial = ser.readline()
		cookedserial = rawserial.decode('utf-8').strip('\r\n')
		temp_val = cookedserial[32:34]
		hum_val = cookedserial[11:13]
		print(temp_val)
		print(hum_val)

		publish.single("Teensy/Temperature", temp_val, hostname="3.25.99.2")
		publish.single("Teensy/Humidity", hum_val, hostname="3.25.99.2")
